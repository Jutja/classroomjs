<html lang="en"><head>
  <meta charset="utf-8">
  <meta http-equiv="X-UA-Compatible" content="IE=edge">
  <meta name="viewport" content="width=device-width, initial-scale=1">
  <title>Classroomjs for GitHub</title>
  <link rel="stylesheet" media="all" href="/css/home.css">
  <link rel="stylesheet" href="/vendor/bower_components/octicons/octicons/octicons.css">
</head>
<body class="site pages home">
  <main>
  <header class="site-header">
    <div class="container">
      <a href="/"><img class="site-logo" alt="GitHub for Classrooms" src="/images/logo@2x.png"></a>
      <nav class="site-nav">
        <ul>
          <li>
            <a target="_blank" href="https://education.github.com">GitHub Education</a>
          </li>
          <li><a href="/login">Sign in</a></li>
        </ul>
      </nav>
    </div></header>
    <div class="site-content">
      <div class="container">
        <div class="site-content">
          <div class="site-content-cap">
            <a class="btn btn-green right" role="button" href="/classroom/new">
              Add Classroom
            </a>    <h2 class="site-content-heading">Classrooms</h2>
          </div>
          <div class="site-content-body organization-list-items">
            
            <div class="flex-table organization-list-item">
              <div class="flex-table-item">
                <img class="avatar left" height="60" width="60" alt="@Jutja" src="https://avatars.githubusercontent.com/u/11042439?v=3&amp;size=120">
                <a class="organization-title" href="/classroom/:id">
                  <strong class="css-truncate-target">Classroom Name</strong>
                </a>
                <span class="text-muted css-truncate-target">@organization Name</span>
              </div>
              <div class="flex-table-item organization-stats text-right">
                <span class="organization-stat-item text-muted tooltipped tooltipped-s" aria-label="Individual Assignments">
                  <span class="octicon octicon-person"></span>No. of Individual Assignments
                </span>
                <span class="organization-stat-item text-muted tooltipped tooltipped-s" aria-label="Group Assignments">
                  <span class="octicon octicon-organization"></span>No. of Group Assignments
                </span>
              </div>
            </div>
            
          </div>
        </div>
      </div>
    </div>
    </main>
    
  </body></html>